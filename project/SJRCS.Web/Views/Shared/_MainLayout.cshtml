<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=11;IE=10;IE=9;IE=8;IE=7;IE=EDGE" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>报表采集系统</title>
    <link href="/Plugins/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="/Plugins/bootstrap/css/bootstrap-google.css" rel="stylesheet" />
    <!--[if lte IE 6]>
        <link href="/Plugins/bootstrap/css/bootstrap-ie6.css" rel="stylesheet" />
    <![endif]-->
    <!--[if lte IE 7]>
        <link href="/Plugins/bootstrap/css/ie.css" rel="stylesheet" />
    <![endif]-->
    <link href="/Content/Css/Main.css" rel="stylesheet" />
    <!--[if lt IE 9]>
		<script src="/Plugins/common/html5.js"></script>
	<![endif]-->
</head>
<body>
    <div id="show_dialog" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-header">
            <button id="btnCloseDialog" type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="dialog_head"></h3>
        </div>
        <div id="modalContainer" class="modal-body">
        </div>
        <div id="buttonContainer" class="modal-footer">
        </div>
    </div>
    <div id="show_message" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-header">
            <button id="btnCloseMessage" type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="message_head"></h3>
        </div>
        <div class="modal-body">
            <p id="message_content"></p>
        </div>
        <div class="modal-footer" id="message_bottom">
        </div>
    </div>
    @RenderBody()
    <script src="/Plugins/common/jquery.js"></script>
    <script src="/Plugins/bootstrap/js/bootstrap.js"></script>
    <!--[if lte IE 6]>
        <script src="/Plugins/bootstrap/js/bootstrap-ie.js"></script>
    <![endif]-->
    <script src="/Plugins/blockUI/jquery.blockUI.js"></script>
    <script src="/Plugins/common/Common.js"></script>
    <script src="/Scripts/Shared/MainLayout.js"></script>
    @RenderSection("Script", false)
</body>
</html>
